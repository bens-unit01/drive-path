<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true" 
    	  android:state_window_focused="false"
        android:drawable="@drawable/btn_dash_on" />
        
    <item android:state_checked="false" 
    	android:state_window_focused="false"
        android:drawable="@drawable/btn_dash_off"/>
      
    <item android:state_checked="true" 
    	android:state_pressed="true"
        android:drawable="@drawable/btn_dash_on_pressed"/>
        
    <item android:state_checked="false" 
    	android:state_pressed="true"
        android:drawable="@drawable/btn_dash_off_pressed"/>
        
    <item android:state_checked="true"
    	android:state_focused="true"
        android:drawable="@drawable/btn_dash_on_selected"/>
        
    <item android:state_checked="false" 
    	android:state_focused="true"
        android:drawable="@drawable/btn_dash_off_selected"/>
    
    <item  
    	android:state_checked="true" 
    	android:state_enabled="false"
    	android:drawable="@drawable/btn_dash_on_disabled"/>	 
        
    <item android:state_checked="false" 
    	android:state_enabled="false" 
    	android:drawable="@drawable/btn_dash_off_disabled"/>
    	
    <item android:state_checked="false" 
    	android:drawable="@drawable/btn_dash_off"/>
    	
    <item android:state_checked="true" 
    	android:drawable="@drawable/btn_dash_on"/>
</selector>



